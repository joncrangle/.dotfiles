{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",
  "experimental": {
    "auto_resume": false,
    "dcp_on_compaction_failure": true
  },
  "disabled_hooks": ["startup-toast"],
  "google_auth": false,
  "agents": {
    "Sisyphus": {
      "model": "google/gemini-claude-opus-4-5-thinking-high",
      "default_builder_enabled": true
    },
    "Planner-Sisyphus": {
      "model": "google/gemini-3-pro-high",
      "default_builder_enabled": true
    },
    "OpenCode-Builder": {
      "model": "google/gemini-claude-sonnet-4-5-thinking-low"
    },
    "oracle": {
      "model": "google/gemini-claude-sonnet-4-5-thinking-high"
    },
    "librarian": {
      "model": "google/gemini-3-flash"
    },
    "frontend-ui-ux-engineer": {
      "model": "google/gemini-3-pro-high"
    },
    "document-writer": {
      "model": "google/gemini-3-flash"
    },
    "multimodal-looker": {
      "model": "google/gemini-3-flash"
    }
  }
}
